{
  "name": "foxy",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "scss": "./dist/assets/scss/main.scss",
      "style": "./dist/assets/css/main.css",
      "default": "./dist/foxy.mjs"
    },
    "./styles": {
      "scss": "./dist/assets/scss/main.scss",
      "style": "./dist/assets/css/main.css"
    },
    "./foxy": "./dist/foxy.js",
    "./components": "./dist/components/index.mjs",
    "./components/*": "./dist/components/*/index.mjs",
    "./composables": "./dist/composables/index.mjs",
    "./directives": "./dist/directives/index.mjs",
    "./enums": "./dist/enums/index.mjs",
    "./consts": "./dist/consts/index.mjs",
    "./types": "./dist/types/index.d.ts",
    "./interfaces": "./dist/interfaces/index.d.ts",
    "./services": "./dist/services/index.mjs",
    "./utils": "./dist/utils/index.mjs",
    "./*": "./dist/*"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "npm run story:dev && npm run server:dev",
    "build": "npm run server:build && npm run story:build",
    "preview": "npm run story:preview && npm run server:preview",

    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",

    "server:dev": "vite",
    "server:build": "unbuild",
    "server:preview": "vite preview",

    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",

    "type:build": "vue-tsc --build --force",
    "type:dev": "vue-tsc --watch --noEmit --skipLibCheck",

    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",

    "test:unit": "vitest",
    "test:e2e": "cypress",
    "test:open": "cypress open",

    "clean": "rm -rf dist node_modules package-lock.json"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^1.0.0-alpha.2",
    "@types/jest": "^29.5.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "histoire": "^1.0.0-alpha.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "markdown-it-codetabs": "^1.2.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "sass": "^1.87.0",
    "shiki": "^3.4.0",
    "ts-transformer-keys": "^0.4.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "unbuild": "^3.5.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  }
}
